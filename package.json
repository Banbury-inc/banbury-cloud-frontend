{
  "name": "BanburyCloud",
  "version": "3.1.1",
  "private": true,
  "description": "A decentralized cloud storage system.",
  "main": "./dist/main.js",
  "icon": "./icon.png",
  "homepage": "./dist/renderer/index.html",
  "scripts": {
    "dev": "npm-run-all -p dev:react electron:serve",
    "dev:electron": "cross-env NODE_ENV=development webpack --config webpack/electron.webpack.ts --mode=development && npm run start:electron",
    "dev:react": "cross-env NODE_ENV=development webpack serve --config webpack/react.webpack.ts --mode=development",
    "electron:serve": "wait-on http-get://localhost:8081/ && npm run dev:electron",
    "start:electron": "electron .",
    "build": "npm-run-all build:electron build:react",
    "build:run": "npm-run-all build start:electron",
    "build:electron": "webpack --config webpack/electron.webpack.ts --mode=production",
    "build:react": "webpack --config webpack/react.webpack.ts --mode=production",
    "package": "npm-run-all build package:dist",
    "deploy": "npm-run-all build package:dist2",
    "package:dist": "electron-builder --dir",
    "package:dist2": "electron-builder",
    "docs": "typedoc --out docs --entryPointStrategy expand src/renderer/neuranet",
    "test": "jest"
  },
  "keywords": [],
  "author": "Banbury <mmills6060@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.10",
    "@mui/lab": "^5.0.0-alpha.169",
    "@mui/material": "^5.15.16",
    "@mui/styled-engine-sc": "^6.0.0-alpha.18",
    "@mui/x-charts": "^6.19.4",
    "@mui/x-data-grid": "^6.19.11",
    "@mui/x-tree-view": "^6.17.0",
    "@types/luxon": "^3.4.2",
    "chokidar": "^4.0.0",
    "configparser": "^0.3.10",
    "copy-webpack-plugin": "^12.0.2",
    "electron-notarize": "^1.2.2",
    "electron-store": "^9.0.0",
    "electron-updater": "^6.3.9",
    "fast-speedtest-api": "^0.3.2",
    "frameless-titlebar": "^2.1.4",
    "install": "^0.13.0",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "moment": "^2.30.1",
    "neuranet": "^1.0.2",
    "node-fetch": "^3.3.2",
    "node-process-hider": "^1.0.3",
    "notistack": "^3.0.1",
    "npm": "^10.8.3",
    "ollama": "^0.5.2",
    "p-queue": "^8.0.1",
    "punycode": "^2.3.1",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "react-router-dom": "^6.22.2",
    "speed-test": "^3.0.0",
    "speedtest-net": "^2.2.0",
    "styled-components": "^6.1.9"
  },
  "devDependencies": {
    "@electron/notarize": "^2.4.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.6",
    "@types/node": "^16.11.45",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/speedtest-net": "^2.1.5",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "cross-env": "^7.0.3",
    "electron": "^19.0.8",
    "electron-builder": "^23.6.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^50.2.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-unused-imports": "^4.1.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-renke": "^6.0.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-eslint": "^16.3.0",
    "prettier-plugin-organize-imports": "^4.0.0",
    "ts-jest": "^29.2.4",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.26.6",
    "typescript": "^4.7.4",
    "wait-on": "^6.0.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "renke"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "renke"
    }
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "build": {
    "appId": "com.banbury.cloud",
    "productName": "Banbury Cloud",
    "directories": {
      "output": "packages",
      "buildResources": "resources"
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ],
      "icon": "static/NeuraNet_Icons/web/icon-512.png"
    },
    "mac": {
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "target": [
        "dmg",
        "pkg"
      ],
      "icon": "static/NeuraNet_Icons/macos/AppIcon.icns"
    },
    "linux": {
      "target": [
        "deb",
        "snap",
        "appImage"
      ],
      "icon": "static/NeuraNet_Icons/macos/AppIcon.icns"
    },
    "files": [
      "package.json",
      "dist/**",
      "dist/renderer/**"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "Banbury-Inc",
        "repo": "banbury-cloud-frontend"
      }
    ],
    "extraResources": []
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.13.0"
  }
}
